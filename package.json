{
  "name": "my-zustand-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run web:dev\"",
    "web:dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "server:dev": "tsx watch server/index.ts",
    "db:generate": "DATABASE_URL=db.sqlite kysely-codegen --out-file server/types.d.ts",
    "db:migrate": "kysely migrate latest && npm run db:generate",
    "server:start": "tsx server/index.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-router": "^1.121.34",
    "@tanstack/router-vite-plugin": "^1.121.37",
    "axios": "^1.10.0",
    "better-sqlite3": "^12.2.0",
    "concurrently": "^9.2.0",
    "fastify": "^5.4.0",
    "kysely": "^0.28.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^4.17.17",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "kysely-codegen": "0.10.1",
    "kysely-ctl": "^0.13.1",
    "prettier": "^3.6.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
